  "dependencies": {
    "lodash": "^4.17.2",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint-config-homeoffice": "^2.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.1.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "snyk": "^1.14.3"
  }
