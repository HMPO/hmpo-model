{
  "name": "hmpo-model",
  "version": "2.1.3",
  "description": "Simple model for interacting with http/rest apis.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run unit",
    "posttest": "npm run cover && npm run check-coverage",
    "lint": "eslint ./",
    "unit": "mocha test/ --recursive --require test/helper",
    "cover": "istanbul cover _mocha -- test/ --recursive --require test/helper --reporter dot",
    "check-coverage": "istanbul check-coverage",
    "snyk": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOffice/passports-model.git"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/UKHomeOffice/passports-model/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/passports-model",
  "dependencies": {
    "hmpo-logger": "^3.0.0",
    "lodash.kebabcase": "^4.1.1",
    "request": "^2.72.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.6.1",
    "istanbul": "^0.4.3",
    "mocha": "^5.0.4",
    "proxyquire": "^2.0.0",
    "sinon": "^4.4.5",
    "sinon-chai": "^3.0.0",
    "snyk": "^1.24.5"
  }
