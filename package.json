{
  "name": "hmpo-model",
  "version": "1.0.0",
  "description": "Simple model for interacting with http/rest apis.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run unit",
    "posttest": "npm run cover && npm run check-coverage",
    "lint": "eslint ./",
    "unit": "mocha test/spec/ --recursive --require test/helper",
    "cover": "istanbul cover _mocha -- test/spec/ --recursive --require test/helper --reporter dot",
    "check-coverage": "istanbul check-coverage",
    "snyk": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOffice/passports-model.git"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/UKHomeOffice/passports-model/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/passports-model",
  "dependencies": {
    "request": "^2.72.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "istanbul": "^0.4.3",
    "mocha": "^2.1.0",
    "proxyquire": "^1.8.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "snyk": "^1.24.5"
  }
}
